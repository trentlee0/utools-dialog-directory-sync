{
  "pluginName": "对话框目录同步",
  "author": "Trent0",
  "homepage": "https://github.com/trentlee0/utools-dialog-directory-sync",
  "description": "同步文件资源管理器、访达窗口的目录路径到文件对话框上，类似于 Listary 的 Ctrl + G 快速切换",
  "version": "1.0.0",
  "logo": "logo.png",
  "preload": "preload.js",
  "platform": [
    "win32",
    "darwin"
  ],
  "features": [
    {
      "code": "switch",
      "explain": "同步当前目录到文件对话框",
      "cmds": [
        "快速切换",
        "同步对话框目录",
        "Quick Switch",
        "Sync Dialog Directory",
        {
          "type": "window",
          "label": "同步对话框目录",
          "match": {
            "class": [
              "#32770"
            ]
          }
        }
      ]
    }
  ]
}